<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AkunActivity">

    <TextView
        android:text="DETAIL AKUN"
        android:layout_width="207dp"
        android:layout_height="59dp"
        android:id="@+id/dashboard"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="normal|bold"
        tools:layout_editor_absoluteY="134dp"
        tools:layout_editor_absoluteX="38dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="23dp">
    </TextView>

    <TextView
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:id="@+id/name"
        android:layout_below="@id/dashboard"
        android:text="Nama          "
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        android:textColor="#000"
        android:layout_marginTop="20dp">
    </TextView>

    <TextView
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:id="@+id/nima"
        android:layout_below="@id/name"
        android:text="NIM              "
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp"
        android:textColor="#000">
    </TextView>

    <TextView
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:id="@+id/email"
        android:layout_below="@id/nima"
        android:text="Email   "
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp"
        android:textColor="#000">
    </TextView>

    <TextView
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:id="@+id/alamat"
        android:layout_below="@id/email"
        android:text="Alamat   "
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp"
        android:textColor="#000">
    </TextView>

    <TextView
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:id="@+id/hp"
        android:layout_below="@id/alamat"
        android:text="No HP   "
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp"
        android:textColor="#000">
    </TextView>

    <TextView
        android:id="@+id/hasil_nama"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_below="@id/dashboard"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="96dp"
        android:text="Nama          "
        android:textColor="#000"
        android:textSize="20dp">
    </TextView>

    <TextView
        android:id="@+id/hasil_nim"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_below="@id/name"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="98dp"
        android:text="NIM              "
        android:textColor="#000"
        android:textSize="20dp">
    </TextView>

    <TextView
        android:id="@+id/hasil_email"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_below="@id/nima"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="98dp"
        android:text="Email   "
        android:textColor="#000"
        android:textSize="20dp">
    </TextView>

    <TextView
        android:id="@+id/hasil_alamat"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_below="@id/hasil_email"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="98dp"
        android:text="Alamat   "
        android:textColor="#000"
        android:textSize="20dp">
    </TextView>

    <TextView
        android:id="@+id/hasil_hp"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_below="@id/hasil_alamat"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="98dp"
        android:text="No HP   "
        android:textColor="#000"
        android:textSize="20dp">
    </TextView>
</RelativeLayout>